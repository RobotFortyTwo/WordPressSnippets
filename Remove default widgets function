<?php 
function unregister_default_widgets() 
{ 
	unregister_widget('WP_Widget_Pages'); // Remove Pages Widget
	unregister_widget('WP_Widget_Calendar');  // Add
	unregister_widget('WP_Widget_Archives');  // Add
	unregister_widget('WP_Widget_Links');  // Remove Links Widget
	unregister_widget('WP_Widget_Meta');  // Remove Meta Widget
	unregister_widget('WP_Widget_Search');  // Remove Search Widget
	unregister_widget('WP_Widget_Text');  // Remove Text Widget
	unregister_widget('WP_Widget_Categories');  // Remove Categories Widget
	unregister_widget('WP_Widget_Recent_Posts');  // Remove Recent Posts Widget
	unregister_widget('WP_Widget_Recent_Comments');  // RemoveRecent Comments Widget
	unregister_widget('WP_Widget_RSS');  // Remove RSS Widget
	unregister_widget('WP_Widget_Tag_Cloud');  // Remove Tag Cloud Widget
	unregister_widget('WP_Nav_Menu_Widget');  // Remove Menus Widget 
}
add_action('widgets_init', 'unregister_default_widgets', 11);
?>
